package  {		public class AdvertObject {		public function AdvertObject(_vidId:int,									 _userId:int,									 _clipname:String,									 _trust:int,									 _pride:int,									 _success:int,									 _passion:int,									 _surprise:int,									 _fear:int,									 _media:String,									 _beginning:String,									 _middle:String,									 _fin:String									 ) 		{			id = _vidId;			user_id = _userId;			clipname = _clipname;			trust = _trust;			pride = _pride;			success = _success;			passion = _passion;			surprise = _surprise;			fear = _fear;			media = _media;			beginning = _beginning;			middle = _middle;			fin = _fin;					}				public var id:int;		public var user_id:int;		public var clipname:String;		public var trust:int;		public var pride:int;		public var success:int;		public var passion:int;		public var surprise:int;		public var fear:int;		public var media:String;		public var beginning:String;		public var middle:String;		public var fin:String;					//--------------------------------------		//  GETTER/SETTERS		//--------------------------------------				public function get getID():int		{			return id;		}				public function get getUserID():int 		{			return user_id;		}				public function get getClipName():String		{			return clipname;		}				public function get getTrust():int		{			return trust;		}				public function get getPride():int		{			return pride;		}				public function get getSuccess():int		{			return success;		}				public function get getPassion():int		{			return passion;		}				public function get getSurprise():int		{			return surprise;		}				public function get getFear():int		{			return fear;		}				public function get getMedia(): String {			return media;		}				public function get getBeginning(): String {			return beginning;		}				public function get getMiddle(): String {			return middle;		}				public function get getFin(): String {			return fin;		}	}	}