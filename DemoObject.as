/* AS3	Copyright 2009 Gali.*/package {		/**	 *	Class description.	 *	 *	@langversion ActionScript 3.0	 *	@playerversion Flash 9.0	 *	 *	@author Sven Gali	 *	@since  01.02.2009	 */	public class DemoObject {				/**		 *	@Constructor		 */		public function DemoObject(									  vidID:int									, myU:String									, myR:String									, myC:String													){			videoID = vidID;			myURL = myU;			myRole = myR;			myCategory = myC;		}				private var videoID:int;		private var myURL:String;		private var myCategory:String;		private var myRole:String;				//--------------------------------------		//  GETTER/SETTERS		//--------------------------------------				public function get getID(): int {			return videoID;		}				public function get getUrl(): String {			return myURL;		}				public function get getMyRole(): String {			return myRole;		}				public function get getMyCategory(): String {			return myCategory;		}	}}