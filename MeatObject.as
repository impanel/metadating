package  {		public class MeatObject {				public function MeatObject(_vidId:int,								   _userId:int,								   _frequency:int,								   _clipName:String,								   _strategy:String,								   _dramaturgy:String,								   _proCoRe:String,								   _tAnr:String,								   _tEnv:String,								   _tHea:String,								   _tLif:String,								   _tEco:String,								   _tTas:String								   ) {						id = _vidId;			user_id = _userId;			frequency = _frequency;			clipname = _clipName;			strategy = _strategy;			dramaturgy = _dramaturgy;			pro_contra_reaction = _proCoRe;			animal_rights = _tAnr;			environment = _tEnv;			healthy = _tHea;			lifestyle = _tLif;			economy = _tEco;			taste = _tTas;						// constructor code		}				//PUBLIC VARIABLES		public var id:int;		public var user_id:int;		public var frequency:int;		public var clipname:String;;		public var strategy:String;		public var dramaturgy:String;		public var pro_contra_reaction:String;		public var animal_rights:String;		public var environment:String;		public var healthy:String;		public var lifestyle:String;		public var economy:String;		public var taste:String;				//--------------------------------------		//  GETTER/SETTERS		//--------------------------------------				public function get getID(): int {			return id;		}				public function get getUserID(): int {			return user_id;		}				public function get getFrequency(): int {			return frequency;		}				public function get getClipName(): String {			return clipname;		}				public function get getStrategy(): String {			return strategy;		}				public function get getDramaturgy(): String {			return dramaturgy;		}				public function get getProContraReaction(): String {			return pro_contra_reaction;		}				public function get getAnimalRights(): String {			return animal_rights;		}				public function get getEnvironment(): String {			return environment;		}				public function get getHealthy(): String {			return healthy;		}				public function get getLifestyle(): String {			return lifestyle;		}				public function get getEconomy(): String {			return economy;		}				public function get getTaste(): String {			return taste;		}			}	}