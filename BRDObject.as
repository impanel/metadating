package  {		public class BRDObject {				/*		xml.tag!(:subgroup, subgroup)       xml.tag!(:clipname, clipname)       xml.tag!(:dramaturgy, dramaturgy)       xml.tag!(:role, role)       xml.tag!(:strategy, strategy)       xml.tag!(:play_pos, play_pos)       xml.tag!(:shot, shot)       xml.tag!(:combination, combination)       xml.tag!(:society, society)       xml.tag!(:worldview, worldview)             xml.tag!(:culture, culture)       xml.tag!(:government, government)		*/		public function BRDObject(_vidId:int,								   _subGroup:String,								   _clipName:String,								   _dramaturgy:String,								   _role:String,								   _strategy:String,								   _nextAr:String,								   _nextAh:String,								   _nextAv:String,								   _nextSb:String,								   _playPos:String,								   _shot:String,								   _combination:String,								   _society:String,								   _worldView:String,								   _culture:String,								   _government:String								   ) 		{						vidId = _vidId;			subGroup = _subGroup;		   	clipName = _clipName;		   dramaturgy = _dramaturgy;		   role = _role;		   strategy = _strategy;		   nextAr = _nextAr;		   nextAh = _nextAh;		   nextAv = _nextAv;		   nextSb = _nextSb;		   playPos = _playPos;		   shot = _shot;		   combination = _combination;		   society = _society;		   worldView = _worldView;		   culture = _culture;		   government = _government;						// constructor code		}				//PUBLIC VARIABLES		public var vidId:int;		public var subGroup:String;		public var clipName:String;		public var dramaturgy:String;;		public var role:String;		public var strategy:String;		public var nextAh:String;		public var nextAr:String;		public var nextAv:String;		public var nextSb:String;		public var playPos:String;		public var shot:String;		public var combination:String;		public var society:String;		public var worldView:String;		public var culture:String;		public var government:String;				//--------------------------------------		//  GETTER/SETTERS		//--------------------------------------				public function get getID(): int {			return vidId;		}				public function get getSubGroup(): String {			return subGroup;		}				public function get getClipName(): String {			return clipName;		}				public function get getDramaturgy(): String {			return dramaturgy;		}				public function get getRole(): String {			return role;		}				public function get getStrategy(): String {			return strategy;		}				public function get getNextAh(): String {			return nextAh;		}				public function get getNextAr(): String {			return nextAr;		}				public function get getNextAv(): String {			return nextAv;		}				public function get getNextSb(): String {			return nextSb;		}				public function get getPlayPos(): String {			return playPos;		}				public function get getShot(): String {			return shot;		}				public function get getCombination(): String {			return combination;		}				public function get getSociety(): String {			return society;		}				public function get getWorldView(): String {			return worldView;		}				public function get getCulture(): String {			return culture;		}				public function get getGovernment(): String {			return government;		}	}}